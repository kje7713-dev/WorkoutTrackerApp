# Use git repo for certs/profiles
storage_mode("git")

token = ENV["MATCH_GIT_TOKEN"]

git_url(
  token.to_s.empty? ?
    "https://github.com/kje7713-dev/ios-certificates.git" :
    "https://#{token}@github.com/kje7713-dev/ios-certificates.git"
)

git_branch("master")

type("appstore")

app_identifier([ENV["APP_IDENTIFIER"]])

username(ENV["APPLE_ID"])
team_id(ENV["APPLE_TEAM_ID"])

readonly(false)

shallow_clone(false)
clone_branch_directly(false)