# ============================
# fastlane/Matchfile
# ============================

storage_mode("git")

# IMPORTANT: use Ruby interpolation + ENV[]
git_url("https://#{ENV['MATCH_GIT_TOKEN']}@github.com/kje7713-dev/ios-certificates.git")

type("appstore")
readonly(false)

app_identifier([ENV["APP_IDENTIFIER"]])
username(ENV["APPLE_ID"])
team_id(ENV["APPLE_TEAM_ID"])

git_branch("master")
shallow_clone(false)
clone_branch_directly(false)
skip_docs(false)
verbose(false)